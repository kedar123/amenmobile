<h1>New friendship</h1>









<h1>Enter Friend Name Who Invites Another Friend</h1>

  
  <%=  form_tag ('/friendships/invite_friend') do %>
     <p><%= label_tag 'Self Name' %><br/>
     <%= text_field_tag :username %></p>
     <p><%= label_tag 'Friend Name' %><br/>
     <%= text_field_tag :friendname %></p>

    <div><%= submit_tag 'Invite' %></div>
  <% end %>

